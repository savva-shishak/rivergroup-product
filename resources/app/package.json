    "setup": "electron-installer-windows --src dist/pto-win32-x64/ --dest dist/installers/",
    "test": "npm run rebuild && ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "rebuild": "electron-rebuild -f -w fs",
    "dev:el": "npm run rebuild && electron ."
  },
